<h3>Vote on Songs</h3>
<% @votes.each do |vote| %>
	<% song = vote.song %>
		<p><%= song.name %><br />by: <%= song.artist %></p>
		<p><%= link_to "Yes", {:controller => "votes", :action => "create", :vote => { :song_id => song.id, :vote => 1 }}, :method => "post" %> | 
	 	<%= link_to "No", {:controller => "votes", :action => "create", :vote => { :song_id => song.id, :vote => 2 }}, :method => "post" %></p>
	 	</div>
<% end %>